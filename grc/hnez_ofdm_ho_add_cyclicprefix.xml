<block>
  <name>Ho add cyclic prefix</name>
  <key>hnez_ofdm_ho_add_cyclicprefix</key>
  <category>[Hnez OFDM]</category>
  <import>import hnez_ofdm</import>
  <make>hnez_ofdm.ho_add_cyclicprefix($fft_len, $cp_len)</make>
  <param>
    <name>Fft_len</name>
    <key>fft_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Cp_len</name>
    <key>cp_len</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$fft_len</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$(fft_len + cp_len)</vlen>
  </source>
</block>
